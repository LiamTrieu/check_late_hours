{
  "name": "Check late hours",
  "description": "Check late hours newwave",
  "version": "1.0",
  "manifest_version": 3,
  "action": {
    "default_icon": "icon.png"
  },
  "content_scripts": [
    {
      "matches": ["*://portal.newwave.vn/*"],
      "run_at": "document_start",
      "js": ["content.js"]
    }
  ],
  "web_accessible_resources": [{
      "resources": ["injected.js"],
      "matches": ["*://portal.newwave.vn/*"]
  }]
}
